{
  "name": "byjg/imageutil",
  "description": "A wrapper collection for GD library in PHP.",
  "autoload": {
    "psr-4": {
      "ByJG\\ImageUtil\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "license": "MIT",
  "require": {
    "php": ">=8.2 <8.5",
    "ext-gd": "*",
    "meyfa/php-svg": "^v0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10|^11",
    "vimeo/psalm": "^5.9|^6.12",
    "ext-imagick": "*"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm"
  }
}
